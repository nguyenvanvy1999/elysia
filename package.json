{
  "name": "elysia",
  "version": "1.1.0",
  "scripts": {
    "----RUN": "",
    "dev": "bun run --watch src/index.ts",
    "start": "bun src/index.ts",
    "----CHECK": "",
    "format": "bunx @biomejs/biome format --write .",
    "lint": "bunx @biomejs/biome lint --apply .",
    "check": "bunx @biomejs/biome check --apply .",
    "----DATABASE": "",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:view": "drizzle-kit studio",
    "db:seed:auth": "bun src/db/seed/auth.ts",
    "db:seed:setting": "bun src/db/seed/setting.ts",
    "db:seed:translation": "bun src/db/seed/translation.ts",
    "db:seed": "bun db:seed:auth && bun db:seed:setting && db:seed:translation",
    "----TEST": "",
    "test": "bun test:unit",
    "test:unit": "bun test test/unit",
    "test:e2e": "bun test test/e2e --preload test/e2e/setup.ts"
  },
